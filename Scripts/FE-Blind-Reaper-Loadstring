--[[ FDless settings if you want to change something ig ]]
options={["#RIP MyWorld"]={
	stoptime = 1, --fling duration
	allowshiftlock = true, --adds shiftlock (left ctrl)
	clickfling = false, --allow click fling
	highlightflingtargets = true, --highlight fling
	respawntp = 3, --respawn tp mode
		--[[
			0 - stay at spawn
			1 - randomtp close
			2 - behind fake char
			3 - hide body
		]]
	breakjointsmethod = 1, --breakjoints mode
		--[[
			1 - health+breakjoints (recommended)
			2 - health or breakjoints
			3 - breakjoints
		]]
	simrad = true --sets simulation radius
}}

